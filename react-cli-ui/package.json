{
  "name": "react-cli-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "axios": "^1.8.1",
    "body-parser": "^1.20.3",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-iframe": "^1.8.5",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.1"
  },
  "scripts": {
    "start": "PORT=3003 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "prod": "npm run build && node server.js",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3003 && cross-env ELECTRON_START_URL=http://localhost:3003 electron .\"",
    "electron": "electron .",
    "electron-build": "npm run build && electron-builder --publish=never",
    "electron-pack": "electron-builder --dir",
    "electron-dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "main.js",
  "build": {
    "appId": "com.versantus.browseruse",
    "productName": "Browser Use",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main.js",
      "preload.js",
      "server.js"
    ],
    "extraFiles": [
      "main.js",
      "preload.js",
      "server.js"
    ],
    "extraResources": [
      {
        "from": "../",
        "to": "extraResources",
        "filter": [
          "cli.py",
          "requirements.txt"
        ]
      }
    ],
    "directories": {
      "buildResources": "icons"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "category": "Development",
      "icon": "icons/icon.png"
    }
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "sharp": "^0.33.5",
    "wait-on": "^8.0.2"
  }
}
